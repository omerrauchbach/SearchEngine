<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.Controller">
    <children>
        <Label layoutX="155.0" layoutY="128.0" prefHeight="17.0" prefWidth="93.0" text="Documents Path" />
        <Label layoutX="155.0" layoutY="183.0" text="Posting Path" />
        <TextField fx:id="documentPath" layoutX="279.0" layoutY="124.0" />
        <TextField fx:id="postingPath" layoutX="279.0" layoutY="179.0" />
        <Button fx:id="start" layoutX="313.0" layoutY="318.0" mnemonicParsing="false" onAction="#onStart" prefHeight="25.0" prefWidth="137.0" text="Start" />
        <Button fx:id="browse_posting" layoutX="442.0" layoutY="124.0" mnemonicParsing="false" onAction="#onBrowseDoc"  text="Browse" />
        <CheckBox fx:id="stemming" layoutX="155.0" layoutY="227.0" mnemonicParsing="false" text="Stemming" />
        <Button fx:id="reset" layoutX="155.0" layoutY="318.0" mnemonicParsing="false" onAction="#onReset" prefHeight="25.0" prefWidth="129.0" text="Reset" />
        <Button fx:id="displayInv" layoutX="313.0" layoutY="274.0" mnemonicParsing="false" onAction="#onDisplayInv" prefHeight="25.0" prefWidth="137.0" text="Display Dictionary" />
        <Button fx:id="loadInv" layoutX="158.0" layoutY="274.0" mnemonicParsing="false" onAction="#onLoadInv" prefHeight="25.0" prefWidth="129.0" text="Load Dictionary" />
      <Button fx:id="browse_Doc" layoutX="442.0" layoutY="179.0" mnemonicParsing="false" onAction="#onBrowsePosting" text="Browse" />
    </children>
</AnchorPane>
